{
  "name": "gaston-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "build:configure": "npx eas build:configure",
    "build:android:development": "npx eas build --platform android --profile development",
    "build:android:preview": "npx eas build --platform android --profile preview",
    "build:android:production": "npx eas build --platform android --profile production",
    "build:ios:development": "npx eas build --platform ios --profile development",
    "build:ios:preview": "npx eas build --platform ios --profile preview",
    "build:ios:production": "npx eas build --platform ios --profile production",
    "build:all:preview": "npx eas build --platform all --profile preview",
    "build:all:production": "npx eas build --platform all --profile production",
    "submit:android": "npx eas submit --platform android",
    "submit:ios": "npx eas submit --platform ios"
  },
  "dependencies": {
    "@expo/cli": "^54.0.5",
    "@expo/vector-icons": "^15.0.2",
    "@tanstack/react-query": "^5.0.0",
    "expo": "~54.0.0",
    "expo-dev-client": "~6.0.12",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.4",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "expo-font": "~14.0.8",
    "@expo/metro-runtime": "~6.1.2",
    "expo-constants": "~18.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
